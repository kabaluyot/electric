    "stylelint": "^8.4.0",
    "stylelint-config-recommended": "^2.1.0",
    "typescript": "~3.5.3",
    "vue-cli-plugin-electron-builder": "^1.4.4",
    "vue-cli-plugin-stylelint": "0.0.2",
    "vue-devtools": "^5.1.3",
    "vue-template-compiler": "^2.6.10"
